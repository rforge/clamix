citHeader("To cite package 'clamix' in publications use:") 
desc <- packageDescription("clamix") 
year <- 2014 
citEntry(entry="unpublished", 
	title = "Clamix – an R-package for custering modal multi valued symbolic data", 
	author = personList(as.person("Vladimir Batagelj"),
	    as.person("Natasa Kejzar"), 
		as.person("Simona Korenjak-Cerne")), 
	year = year, 
	note = "in preparation",
	textVersion = 
		paste("V.Batagelj, N. Kejzar, S. Korenjak-Cerne (", year, "). Clamix – R-package for custering modal multi valued symbolic data", sep="")
)
